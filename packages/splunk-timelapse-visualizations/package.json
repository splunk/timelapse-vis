  "license": "UNLICENSED",
  "scripts": {
    "build": "NODE_ENV=production webpack --bail",
    "build_dev": "NODE_ENV=development webpack --bail",
    "eslint": "eslint src --ext \".js,.jsx\"",
    "eslint:ci": "yarn run eslint -f junit -o test-reports/lint-results.xml",
    "eslint:fix": "eslint src --ext \".js, .jsx\" --fix",
    "link:app": "ln -s $PWD/stage $SPLUNK_HOME/etc/apps/splunk-timelapse-visualizations",
    "lint": "yarn run eslint && yarn run stylelint",
    "lint:ci": "yarn run eslint:ci && yarn run stylelint",
    "start": "webpack --watch",
    "stylelint": "stylelint \"src/**/*.{js,jsx}\" --config stylelint.config.js"
  },
  "dependencies": {
    "@splunk/splunk-utils": "^2.1.0",
    "css-loader": "5.2.7",
    "style-loader": "2.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.0",
    "@splunk/dashboard-context": "^23.1.0",
    "@splunk/dashboard-core": "^23.1.0",
    "@splunk/dashboard-presets": "^23.1.0",
    "@splunk/eslint-config": "^4.0.0",
    "@splunk/rangesliderinput": "^0.0.1",
    "@splunk/react-page": "^5.1.0",
    "@splunk/splunk-utils": "^2.1.0",
    "@splunk/themes": "^0.9.0",
    "@splunk/timelapseinput": "^0.0.1",
    "@splunk/webpack-configs": "^5.0.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.0.4",
    "copy-webpack-plugin": "^4.5.2",
    "eslint": "^7.14.0",
